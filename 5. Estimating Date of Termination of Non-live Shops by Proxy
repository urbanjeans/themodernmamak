a/\"\n","\n","decades = [\"1980s\", \"1990s\", \"2000s\", \"2010s\", \"2020s\"]\n","\n","mamaks_geo = {decade: pd.read_csv(f\"{base_path}mamaks_{decade}_nlive.csv\") for decade in decades}\n","\n","mm_1980s_nlive = mamaks_geo[\"1980s\"]\n","mm_1990s_nlive = mamaks_geo[\"1990s\"]\n","mm_2000s_nlive = mamaks_geo[\"2000s\"]\n","mm_2010s_nlive = mamaks_geo[\"2010s\"]\n","mm_2020s_nlive = mamaks_geo[\"2020s\"]"],"metadata":{"id":"rP12ZE3lg-KT","executionInfo":{"status":"ok","timestamp":1744798188310,"user_tz":-480,"elapsed":2553,"user":{"displayName":"Wueng Kee Ong","userId":"15013982952133316447"}}},"execution_count":4,"outputs":[]},{"cell_type":"markdown","source":["# Combine All Non-live Shops"],"metadata":{"id":"foqYumk9VhAZ"}},{"cell_type":"code","source":["mm_all_nlive = pd.concat([mm_1980s_nlive, mm_1990s_nlive, mm_2000s_nlive, mm_2010s_nlive, mm_2020s_nlive], ignore_index=True)\n","mm_all_nlive = mm_all_nlive.reset_index(drop=True)"],"metadata":{"id":"l5jcSreJXXmQ","executionInfo":{"status":"ok","timestamp":1744798188324,"user_tz":-480,"elapsed":9,"user":{"displayName":"Wueng Kee Ong","userId":"15013982952133316447"}}},"execution_count":5,"outputs":[]},{"cell_type":"code","source":["mm_all_nlive.info()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"zkWUco9LZVVL","executionInfo":{"status":"ok","timestamp":1744798188364,"user_tz":-480,"elapsed":35,"user":{"displayName":"Wueng Kee Ong","userId":"15013982952133316447"}},"outputId":"76b8f7dc-89d6-4200-961d-5acbf453b088"},"execution_count":6,"outputs":[{"output_type":"stream","name":"stdout","text":["<class 'pandas.core.frame.DataFrame'>\n","RangeIndex: 4519 entries, 0 to 4518\n","Data columns (total 31 columns):\n"," #   Column                             Non-Null Count  Dtype  \n","---  ------                             --------------  -----  \n"," 0   uen                                4519 non-null   object \n"," 1   issuance_agency_id                 4519 non-null   object \n"," 2   entity_nam