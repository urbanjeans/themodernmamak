ered within the period\n","    shops_tdec = mm_all_nlive[\n","        (mm_all_nlive['registration_year'] >= start_yr) &\n","        (mm_all_nlive['registration_year'] <= end_yr)\n","    ]\n","\n","    #shops registered before the period but still open within the period\n","    shops_pre_tdec_open_tdec = mm_all_nlive[\n","        (mm_all_nlive['registration_year'] < start_yr) &\n","        (mm_all_nlive['termination_year'] > end_yr)\n","    ]\n","\n","    #combine and remove duplicates based on entity_name\n","    result = pd.concat([shops_tdec, shops_pre_tdec_open_tdec]).drop_duplicates(subset=['entity_name']).reset_index(drop=True)\n","\n","    return result\n"],"metadata":{"id":"X6XpoY-Elapu"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["mm_live_atm_1990s = live_shops_atm(1990, 1999)\n","mm_live_atm_1990s.info()"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"LwYB3pXCUlc-","executionInfo":{"status":"ok","timestamp":1744459551736,"user_tz":-480,"elapsed":56,"user":{"displayName":"Wueng Kee Ong","userId":"15013982952133316447"}},"outputId":"3f84c630-59b0-4632-b63b-f665b403bec1"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["<class 'pandas.core.frame.DataFrame'>\n","RangeIndex: 1719 entries, 0 to 1718\n","Data columns (total 32 columns):\n"," #   Column                             Non-Null Count  Dtype  \n","---  ------                             --------------  -----  \n"," 0   uen                                1719 non-null   object \n"," 1   issuance_agency_id                 1719 non-null   object \n"," 2   entity_name                        1719 non-null   object \n"," 3   entity_type_description            1719 non-null   object \n"," 4   business_constitution_description  1719 non-null   object \n"," 5   company_type_description           1719 non-null   object \n"," 6   paf_constitution_description       1719 non-null   object \n"," 7   entity_status_description          1719 non